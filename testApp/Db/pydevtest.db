record(longout, "PyDev:B_SP") {
  field(DTYP, "pydev")
  field(OUT,  "@pydevtest.b=%value%")
  field(VAL,  "1")
  field(PINI, "YES")
  field(TPRO, "1")
  field(FLNK, "PyDev:B_RB")
}
